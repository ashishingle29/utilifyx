    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "echo \"Add linter\"",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "@types/jest": "^29.0.0"
  }
